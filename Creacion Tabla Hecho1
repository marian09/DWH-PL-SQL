CREATE TABLE HECHO1 
    ( 
     SOLICITUD_ID NUMBER  NOT NULL , 
     ESTADO_ID NUMBER  NOT NULL , 
     CENTRO_ID NUMBER  NOT NULL , 
     FECHA_I_ID NUMBER  NOT NULL , 
     FECHA_F_ID NUMBER  NOT NULL , 
     TIEMPO_ATENCION NUMBER , 
     CANT_SOLIC_EDO NUMBER , 
     TIEM_PROM_SOL FLOAT , 
     CANTIDAD_TOTAL NUMBER , 
     CANTIDAD_SOLC_CENTRO NUMBER 
    ) 
;



ALTER TABLE HECHO1 
    ADD CONSTRAINT HECHO1_CENTRO_COSTOS_FK FOREIGN KEY 
    ( 
     CENTRO_ID
    ) 
    REFERENCES CENTRO_COSTOS 
    ( 
     CENTRO_ID
    ) 
;


ALTER TABLE HECHO1 
    ADD CONSTRAINT HECHO1_ESTADO_FK FOREIGN KEY 
    ( 
     ESTADO_ID
    ) 
    REFERENCES ESTADO 
    ( 
     ESTADO_ID
    ) 
;


ALTER TABLE HECHO1 
    ADD CONSTRAINT HECHO1_FECHA_FIN_FK FOREIGN KEY 
    ( 
     FECHA_F_ID
    ) 
    REFERENCES FECHA_FIN 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO1 
    ADD CONSTRAINT HECHO1_FECHA_INI_FK FOREIGN KEY 
    ( 
     FECHA_I_ID
    ) 
    REFERENCES FECHA_INI 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO1 
    ADD CONSTRAINT HECHO1_SOLICITUD_FK FOREIGN KEY 
    ( 
     SOLICITUD_ID
    ) 
    REFERENCES SOLICITUD 
    ( 
     SOLICITUD_ID
    ) 
;

